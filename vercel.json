{
  "rewrites": [
    {
      "source": "/app.html/:path*",
      "destination": "/app.html"
    },
    {
      "source": "/app/:path*",
      "destination": "/app.html"
    },
    {
      "source": "/checkout/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/subscriptions",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
