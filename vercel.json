{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "shop.mmagym.be"
        }
      ],
      "destination": "/app.html"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "crm.mmagym.be"
        }
      ],
      "destination": "/app.html"
    },
    {
      "source": "/app.html/(.*)",
      "destination": "/app.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
